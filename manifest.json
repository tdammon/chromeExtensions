{
    "manifest_version": 2,
    "name": "Chrome Extension",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js":["jquery.js","content.js"]
        }
    ],
    "permissions": ["tabs"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "Frog-1.png",
        "default_popup": "popup/index.html"
    }
}